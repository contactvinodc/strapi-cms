{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "summary": {
      "type": "text",
      "maxLength": 300
    },
    "posted_at": {
      "type": "date"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "maxLength": 200
    },
    "sub_title": {
      "type": "string"
    }
  }
}
